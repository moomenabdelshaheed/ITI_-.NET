<table class=" table table-hover table-bordered">

	<tr>
		<th>
			ID
		</th>
		<th>
			NAME
		</th>
		<th>
			Image
		</th>
	</tr>
		@foreach(Student item in Model){
		<tr>

			<td>@item.Id</td>
			<td>@item.Name</td>
			<td>@item.ImageUrl</td>
			<td><img src="~/lib/" /></td>
		</tr>

		}
</table>